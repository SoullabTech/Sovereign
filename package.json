    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:studio": "prisma studio",
    "db:seed": "tsx prisma/seed.ts",
    "typecheck": "tsc --noEmit",
    "pwa:build": "npm run build && npm run pwa:generate-icons",
    "pwa:optimize": "node scripts/optimize-pwa.js",
    "pwa:generate-icons": "npx pwa-asset-generator ./public/logo.svg ./public/icons --background '#6366f1'",
    "analyze": "ANALYZE=true next build",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:e2e": "jest --config jest.e2e.config.js",
    "test:pipeline": "jest tests/sacred-pipeline.test.ts --verbose",
    "test:persistence": "jest tests/supabase-persistence.test.ts --verbose",
    "test:ci": "npm run lint && npm run test:coverage",
    "test:beta": "cd apps/web && npx tsx scripts/test-beta-diagnostics.ts",
    "setup:beta-real": "npx tsx scripts/setup-real-beta.ts",
    "type-check": "tsc --noEmit",
    "docs:fix": "node scripts/docs-fix.js",
    "vault:connect": "tsx scripts/start-obsidian-integration.ts",
    "vault:scan": "node scripts/connect-vault.js",
    "vault:sacred": "tsx scripts/sacred-vault-integration.ts",
    "vault:sacred:simple": "tsx scripts/vault-sacred-simple.ts",
    "vault:configure": "tsx scripts/configure-obsidian-vault.ts",
    "awaken-maya": "tsx lib/orchestration/awaken-maya.ts",
    "maya": "tsx lib/orchestration/awaken-maya.ts",
    "claude-code:server": "tsx server/start.ts",
    "cc:server": "tsx server/start.ts",
    "import:wisdom": "tsx scripts/import-wisdom.ts",
    "test:search": "tsx scripts/test-semantic-search.ts",
    "spiralogic:test": "tsx scripts/test-spiralogic-system.ts",
    "spiralogic:init": "tsx scripts/configure-obsidian-vault.ts && npm run spiralogic:test",
    "consciousness:deploy": "bash deploy-consciousness-computing.sh",
    "consciousness:build": "npm run build && npm run consciousness:sync",
    "consciousness:sync": "npx cap sync ios && npx cap sync android",
    "consciousness:ios": "npm run consciousness:sync && npx cap build ios",
    "consciousness:android": "npm run consciousness:sync && npx cap build android",
    "consciousness:pwa": "NEXT_PUBLIC_CONSCIOUSNESS_PWA=true npm run build",
    "pwa:consciousness": "npm run consciousness:pwa && npm run pwa:generate-icons",
    "ios:consciousness": "npm run consciousness:ios && npx cap open ios",
    "android:consciousness": "npm run consciousness:android && npx cap open android",
    "deploy:all-platforms": "npm run consciousness:deploy"
