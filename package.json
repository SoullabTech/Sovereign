  "build": {
    "appId": "life.soullab.maia.labtools",
    "productName": "MAIA LabTools + IPP",
    "directories": {
      "output": "dist-desktop"
    },
    "files": [
      "out/**/*",
      "electron/**/*",
      "public/icons/**/*",
      "node_modules/**/*",
      "package.json"
    ],
    "extraResources": [
      {
        "from": "docs/testing",
        "to": "docs/testing",
        "filter": [
          "**/*.md"
        ]
      }
    ],
    "mac": {
      "category": "public.app-category.healthcare-fitness",
      "icon": "public/icons/icon-512x512.png",
      "type": "distribution",
      "target": [
        {
          "target": "dmg",
          "arch": [
            "x64",
            "arm64"
          ]
        },
        {
          "target": "zip",
          "arch": [
            "x64",
            "arm64"
          ]
        }
      ],
      "entitlements": "build/entitlements.mac.plist",
      "entitlementsInherit": "build/entitlements.mac.plist",
      "notarize": false
    },
    "dmg": {
      "title": "MAIA LabTools + IPP",
      "backgroundColor": "#1A1513",
      "window": {
        "width": 540,
        "height": 380
      }
    },
    "win": {
      "icon": "public/icons/icon-512x512.png",
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64"
          ]
        },
        {
          "target": "portable",
          "arch": [
            "x64"
          ]
        }
      ]
    },
    "linux": {
      "icon": "public/icons/icon-512x512.png",
      "category": "Science",
      "target": [
        {
          "target": "AppImage",
          "arch": [
            "x64"
          ]
        },
        {
          "target": "deb",
          "arch": [
            "x64"
          ]
        }
      ]
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true,
      "createDesktopShortcut": true,
      "createStartMenuShortcut": true
    }
  }
