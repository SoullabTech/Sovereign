{
  "name": "spiralogic-oracle-system",
  "version": "0.1.0",
  "private": true,
  "main": "electron/main.js",
  "homepage": "./",
  "scripts": {
    "dev": "env -u DATABASE_URL next dev",
    "dev:voice": "bash start-voice.sh",
    "dev:https": "node server.mjs",
    "worker": "env -u DATABASE_URL AIN_WORKER=true npx tsx scripts/start-ain-worker.ts",
    "build": "node scripts/verify-aetheric-consciousness.js && node scripts/validate-critical-files.js && next build",
    "build:docker": "node scripts/verify-aetheric-consciousness.js && node scripts/validate-critical-files.js && next build --webpack",
    "build:sovereign": "bash scripts/sovereign-build.sh",
    "android:build": "bash scripts/build-android.sh debug",
    "android:release": "bash scripts/build-android.sh release",
    "android:bundle": "bash scripts/build-android.sh bundle",
    "ios:build": "bash scripts/build-ios.sh debug",
    "ios:release": "bash scripts/build-ios.sh release",
    "version:bump": "bash scripts/version-bump.sh",
    "version:patch": "bash scripts/version-bump.sh auto patch",
    "version:minor": "bash scripts/version-bump.sh auto minor",
    "version:major": "bash scripts/version-bump.sh auto major",
    "start": "next start",
    "desktop:dev": "concurrently \"npm run dev\" \"wait-on http://localhost:3000/maia/labtools && electron electron/main.js\"",
    "desktop:build": "npm run build && npm run desktop:package",
    "desktop:package": "electron-builder",
    "desktop:start": "electron electron/main.js",
    "lint": "cd apps/web && next lint",
    "check:sovereignty": "tsx scripts/check-maia-sovereignty.ts",
    "check:no-supabase": "tsx scripts/check-no-supabase.ts",
    "validate:critical": "node scripts/validate-critical-files.js",
    "verify:claude": "bash scripts/verify-claude-project.sh",
    "claude:env": "bash scripts/claude-env-setup.sh",
    "audit:sovereignty": "tsx scripts/audit-sovereignty.ts",
    "audit:typehealth": "npm run typecheck -- --pretty false > artifacts/typecheck-full.log 2>&1 || true && tsx scripts/audit-typehealth.ts",
    "audit:artifacts": "tsx scripts/verify-artifact-integrity.ts",
    "audit:artifacts:update": "tsx scripts/verify-artifact-integrity.ts --update",
    "audit:artifacts:check": "tsx scripts/verify-artifact-integrity.ts --check",
    "verify:aetheric": "node scripts/verify-aetheric-consciousness.js",
    "consciousness:check": "node scripts/verify-aetheric-consciousness.js",
    "consciousness:dev": "npm run verify:aetheric && npm run dev",
    "consciousness:seed-rules": "tsx scripts/seed-consciousness-rules.ts",
    "postinstall": "prisma generate",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:studio": "prisma studio",
    "db:seed": "tsx prisma/seed.ts",
    "typecheck": "tsc -p tsconfig.typecheck.json --noEmit",
    "typecheck:scripts": "tsc -p tsconfig.scripts.json --noEmit",
    "preflight": "npm run check:no-supabase && docker compose config >/dev/null && echo 'âœ… Preflight passed: No Supabase + Docker config valid'",
    "biomarkers:report": "tsx scripts/report-biomarker-metrics.ts",
    "biomarkers:verify": "tsx scripts/test-biomarker-exports.ts --log artifacts/phase4.2d-barrel-verification.log",
    "pwa:build": "npm run build && npm run pwa:generate-icons",
    "pwa:optimize": "node scripts/optimize-pwa.js",
    "pwa:generate-icons": "npx pwa-asset-generator ./public/logo.svg ./public/icons --background '#6366f1'",
    "analyze": "ANALYZE=true next build",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "test": "jest --config jest.config.js",
    "test:watch": "jest --config jest.config.js --watch",
    "test:coverage": "jest --config jest.config.js --coverage",
    "test:e2e": "playwright test",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:report": "playwright show-report",
    "test:pipeline": "jest --config jest.config.js tests/sacred-pipeline.test.ts --verbose",
    "test:persistence": "jest --config jest.config.js tests/supabase-persistence.test.ts --verbose",
    "test:ci": "npm run lint && npm run test:coverage",
    "test:beta": "cd apps/web && npx tsx scripts/test-beta-diagnostics.ts",
    "setup:beta-real": "npx tsx scripts/setup-real-beta.ts",
    "type-check": "tsc --noEmit",
    "docs:fix": "node scripts/docs-fix.js",
    "render:diagrams": "bash scripts/render-diagrams.sh",
    "check:diagrams": "bash scripts/check-diagrams.sh",
    "vault:connect": "tsx scripts/start-obsidian-integration.ts",
    "vault:scan": "node scripts/connect-vault.js",
    "vault:sacred": "tsx scripts/sacred-vault-integration.ts",
    "vault:sacred:simple": "tsx scripts/vault-sacred-simple.ts",
    "vault:configure": "tsx scripts/configure-obsidian-vault.ts",
    "awaken-maya": "tsx lib/orchestration/awaken-maya.ts",
    "maya": "tsx lib/orchestration/awaken-maya.ts",
    "claude-code:server": "tsx server/start.ts",
    "cc:server": "tsx server/start.ts",
    "import:wisdom": "tsx scripts/import-wisdom.ts",
    "test:search": "tsx scripts/test-semantic-search.ts",
    "spiralogic:test": "tsx scripts/test-spiralogic-system.ts",
    "spiralogic:init": "tsx scripts/configure-obsidian-vault.ts && npm run spiralogic:test",
    "consciousness:deploy": "bash deploy-consciousness-computing.sh",
    "consciousness:build": "npm run build && npm run consciousness:sync",
    "consciousness:sync": "npx cap sync ios && npx cap sync android",
    "consciousness:ios": "npm run consciousness:sync && npx cap build ios",
    "consciousness:android": "npm run consciousness:sync && npx cap build android",
    "consciousness:pwa": "NEXT_PUBLIC_CONSCIOUSNESS_PWA=true npm run build",
    "pwa:consciousness": "npm run consciousness:pwa && npm run pwa:generate-icons",
    "ios:consciousness": "npm run consciousness:ios && npx cap open ios",
    "android:consciousness": "npm run consciousness:android && npx cap open android",
    "deploy:all-platforms": "npm run consciousness:deploy"
  },
