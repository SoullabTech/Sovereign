# MAIA Staging Environment Configuration
# This runs parallel to production for testing

# Basic Configuration
NODE_ENV=staging
NEXT_PUBLIC_SITE_URL=http://localhost:3010

# Database (use same Supabase instance but different schema if needed)
SUPABASE_URL=your_supabase_url
SUPABASE_ANON_KEY=your_supabase_anon_key
SUPABASE_SERVICE_ROLE_KEY=your_service_role_key

# Redis (staging instance)
REDIS_URL=redis://localhost:6380

# API Keys (use staging/test keys)
ANTHROPIC_API_KEY=your_staging_anthropic_key
OPENAI_API_KEY=your_staging_openai_key

# Feature Flags for Testing
ENABLE_EXPERIMENTAL_FEATURES=true
DEBUG_MODE=true
VERBOSE_LOGGING=true

# Staging-specific settings
STAGING=true
DISABLE_ANALYTICS=true
MOCK_EXTERNAL_APIS=false